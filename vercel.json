{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/search.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
